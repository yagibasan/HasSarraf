  <data name="tileView2.TileHtmlTemplate.Styles" xml:space="preserve">
    <value>

/* Genel container yapısı */
.urunSatir {
  display: flex; 
  align-items: center;
  padding: 0px; 
  border-radius: 8px; 
  font-family: "Consolas";
}

.urunAdi {
  font-weight: bold;
  font-size: 3rem;
  color: White;
  background-color: #7367f0;
  border-radius: 8px;
  padding: 6px 10px; 
  margin-right: 20px;
  max-width: 400px;
  width: 400px;
}

 

.urunAlisContainer{
  display: flex; 
  background-color: #28c76f; 
  font-weight: bold;
   font-size: 3rem;
   color: White; 
   border-radius: 8px;
   padding: 4px 8px; 
   margin-right: 20px; 
   width: 200px;
   position: relative;
}

.urunAlisOrijinal{
 background-color: #99808390;
 border-radius: 8px;
 font-size: 12px;
  padding: 1px 8px;
  white-space: nowrap; 
   
}

.urunAlisKarZarar{
 background-color: #99808390;
 border-radius: 8px;
 font-size: 12px;
  padding: 1px 8px;
  margin-top: 5px; 
  white-space: nowrap; 
}

.urunAlisTabela{  
 font-size: 3rem; 
 text-align: right;
 margin-top: 4px;
 width: 100%;
}

.urunSatisContainer{
  display: flex;
  background-color: #ff4c51; 
  font-weight: bold;
   font-size: 3rem;
   color: White; 
   border-radius: 8px;
   padding: 4px 8px; 
   margin-right: 20px; 
   width: 200px;
}

.urunSatisOrijinal{
 background-color: #99808390;
 border-radius: 8px;
 font-size: 12px;
  padding: 1px 8px;
  white-space: nowrap;  
   
}

.urunSatisKarZarar{
 background-color: #99808390;
 border-radius: 8px;
 font-size: 12px;
  padding: 1px 8px;
  margin-top: 5px; 
  text-align: right;
  white-space: nowrap; 
}

.urunSatisTabela{  
 font-size: 3rem; 
 text-align: left;
 margin-top: 4px;
 width: 100%;
}

 








































































</value>
  </data>
  <data name="tileView2.TileHtmlTemplate.Template" xml:space="preserve">
    <value>&lt;div class="urunSatir"&gt;
  &lt;div class="urunAdi"&gt;
    ${Urun.Adi} 
  &lt;/div&gt;

  &lt;div class="urunAlisContainer"&gt;
   &lt;div&gt;
      &lt;div class="urunAlisOrijinal"&gt;${AlisOrijinalText}&lt;/div&gt;   
      &lt;div class="urunAlisKarZarar"&gt;${FarkAlisText}&lt;/div&gt; 
   &lt;/div&gt;  
   &lt;div class="urunAlisTabela"&gt;${AlisTabelaText}&lt;/div&gt; 
  &lt;/div&gt;

  &lt;div class="urunSatisContainer"&gt;
     
    &lt;div class="urunSatisTabela"&gt;${SatisTabelaText}&lt;/div&gt; 
    &lt;div&gt;
      &lt;div class="urunSatisOrijinal"&gt;${SatisOrijinalText}&lt;/div&gt;   
      &lt;div class="urunSatisKarZarar"&gt;${FarkSatisText}&lt;/div&gt; 
   &lt;/div&gt;  
  
  &lt;/div&gt;

  
&lt;/div&gt;</value>
  </data>
